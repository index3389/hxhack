<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hbsoft.hack.vo">
	<class name="FatieVO" table="fatie">
		<id name="tieid" column="tieid">
			<generator class="native"/>
		</id>
		<property name="title"/>
		<property name="hackName"/>	
		<property name="content"/>
		<property name="type" type="java.lang.Integer"/>
		<property name="xtype" type="java.lang.Integer"/>
		<property name="createTime"/>
		<property name="lasthui"/>
		<property name="count"/>
		<property name="best"/>
		<property name="replay"/>
	
	</class>
</hibernate-mapping>