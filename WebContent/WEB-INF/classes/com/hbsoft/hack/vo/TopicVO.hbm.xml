<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hbsoft.hack.vo">
	<class name="TopicVO" table="topic">
		<id name="topicid">
			<generator class="native"/>
		</id>
		<property name="title"/>
		<property name="author" column="adminName"/>	
		<property name="content"/>
		
		<property name="isnew"/>
		<property name="isbest"/>	
		<property name="type"/>
		<property name="createTime"/>
	
	</class>
</hibernate-mapping>